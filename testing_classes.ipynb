   "source": [
    "#iterate through pedler_data and split on the | character\n",
    "pedler_dict = {}\n",
    "for line in pedler_data:\n",
    "    split_line = line.strip().split(\"|\")\n",
    "    #from split_line remove the numeral string\n",
    "\n",
    "    to_add = []\n",
    "    for i in range(len(split_line)):\n",
    "        if split_line[i].isdigit() or split_line[i] == \"\" or split_line[i] == \" \" or split_line[i].isupper():\n",
    "            continue\n",
    "        if not split_line[i].isdigit() and split_line[i] != \"\":\n",
    "            to_add.append(split_line[i])\n",
    "    print(to_add)\n",
    "    for j in range(len(to_add)):\n",
    "        #add work to dictionary as key with values all other values in to_add\n",
    "        for k in range(len(to_add)):\n",
    "            if k != j:\n",
    "                #check if key already exists\n",
    "                if to_add[j] in pedler_dict:\n",
    "                    #check if value already exists in that key\n",
    "                    if to_add[k] not in pedler_dict[to_add[j]]:\n",
    "                        pedler_dict[to_add[j]].append(to_add[k])\n",
    "                else:\n",
    "                    pedler_dict[to_add[j]] = [to_add[k]]\n",
    "    \n",
    "               "
   ]
